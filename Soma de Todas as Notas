function obterSomaDeTodasAsNotas(notas) {
let soma = 0
notas.forEach(function(nota){
  soma = soma + nota.nota
  
return soma + nota
})
return soma
}

let notas = [
  {
    nome: "Sandra Assis",
    nota: 8
  },
  {
    nome: "Ã‰rica Blanck",
    nota: 3
  }
];

console.log(obterSomaDeTodasAsNotas(notas));
